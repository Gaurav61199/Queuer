<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.gaurav.android.queuer"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".GetArrayList">
        <activity android:name=".Queuer"
            android:label="@string/homeToolbarTitle">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".FormActivity"
            android:label="@string/formToolbarTitle">
        </activity>
        <activity android:name=".FormActivity2"/>
    </application>

</manifest>